@using System.Configuration
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
</head>

<body>
    <div class="navbar navbar-fixed-top">
        <div class="container">

            <ul class="nav navbar-nav navbar-right">
                <li><a style="background-color: inherit" href="~/Cv/GetPdf" id="registerLink"><img border="0" alt="W3Schools" src="~/Content/adobe-pdf-icon.png" width="50" height="50"></a></li>
            </ul>

        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>@ConfigurationManager.AppSettings["Footer"]</p>
        </footer>
    </div>
</body>
</html>
